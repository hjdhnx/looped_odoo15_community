<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="seq_hr_custody_seq" model="ir.sequence">
        <field name="name">Custody Code</field>
        <field name="code">hr.custody</field>
        <field name="prefix">CUS/</field>
        <field name="padding" eval="4"/>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_custody_property_seq" model="ir.sequence">
        <field name="name">Custody Property Code</field>
        <field name="code">custody.property</field>
        <field name="prefix">CUSPRO/</field>
        <field name="padding" eval="6"/>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
