<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.hr</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="70"/>
        <field name="inherit_id" ref="hr.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='work_organization_setting_container']" position="after">


                <h2>Employee Sequance</h2>
                <div class="row mt16 o_settings_container" name="he_employee_sequance">
                    <div class="col-12 col-lg-6 o_setting_box" id="default_company_schedule_setting">
                        <div class="o_setting_right_pane">

                            <span class="o_form_label">Employee Automatic Sequance</span>
                            <div class="content-group" name="hr_auto_seq">
                                <!-- <div class="row">

                                    <field name="hr_employee_seq_start_with" force_save="1" attrs="{'readonly':[('hr_employee_auto_seq','=',True)]}" class="col-lg-2 ml16"/>
                                    <label for="hr_employee_seq_start_with" />

                                </div> -->
                                <div class="row">

                                    <field name="hr_employee_auto_seq" class="col-lg-2 ml16"/>
                                    <label for="hr_employee_auto_seq" />

                                </div>
                            </div>






                        </div>
                    </div>
                </div>


            </xpath>
        </field>
    </record>

</odoo>
